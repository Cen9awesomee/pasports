ctiveModule = this.getModuleByLink(matchingLink.href);\n      } else if (allLinks.length) {\n        const firstLink = allLinks[0];\n        this.activeLink = firstLink.href;\n        this.activeModule = this.getModuleByLink(firstLink.href);\n      }\n    },\n    getModuleByLink(linkHref) {\n      for (const [module, links] of Object.entries(this.moduleLinks)) {\n        if (links.some(link => link.href === linkHref)) {\n          return parseInt(module);\n        }\n      }\n      return 1;\n    },\n    selectLanguage(lang) {\n      this.selectedLanguage = lang;\n    },\n    closeTooltip() {\n      if (this.$refs.langToggle && this.$refs.langToggle._tooltip) {\n        this.$refs.langToggle._tooltip.hide();\n      }\n    },\n    handleDocumentClick(event) {\n      const langToggle = this.$refs.langToggle;\n      const tooltip = document.querySelector('.v-tooltip__content.lang-tooltip');\n      if (!langToggle.contains(event.target) && !(tooltip && tooltip.contains(event.target))) {\n        this.closeTooltip();\n      }\n    }\n  },\n  mounted() {\n    this.initializeActiveLink();\n    window.selectLanguage = this.selectLanguage.bind(this);\n    window.closeTooltip = this.closeTooltip.bind(this);\n\n    // Добавляем на capture-фазе\n    document.addEventListener('click', this.handleDocumentClick, true);\n  },\n  beforeDestroy() {\n    window.selectLanguage = null;\n    window.closeTooltip = null;\n    document.removeEventListener('click', this.handleDocumentClick, true);\n  }\n};","map":{"version":3,"names":["MapCard","name","components","props","backgroundColor","type","String","default","data","activeModule","activeLink","selectedLanguage","tooltipInstance","moduleLinks","href","icon","label","hashMapping","computed","pageStyles","tooltipOptions","content","tooltipContent","placement","html","offset","classes","trigger","hideOnTargetClick","methods","toggleCards","isExpanded","cardContainer","document","querySelector","classList","add","remove","selectModule","mo